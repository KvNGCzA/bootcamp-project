    "babel-preset-airbnb": "^2.5.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "eslint-plugin-import": "^2.14.0",
    "express": "^4.16.3",
    "hbs": "^4.0.1",
    "istanbul-threshold-exit-code": "^1.0.3",
    "jquery": "^3.3.1",
    "jquery-ui": "^1.12.1",
    "jsonwebtoken": "^8.3.0",
    "make-runnable": "^1.3.6",
    "morgan": "^1.9.0",
    "multer": "^1.3.1",
    "nodemon": "^1.18.3",
    "pg": "^7.4.3",
    "pg-promise": "^8.4.6",
    "request": "^2.88.0",
    "swagger-ui": "^3.19.0"
  },
