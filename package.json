{
  "name": "final-project",
  "version": "1.0.0",
  "description": "Two week bootcamp project",
  "main": "server.js",
  "scripts": {
    "start": "babel-node ./server/server.js",
    "nodestart": "nodemon --exec npm run start",
    "mocha": "mocha --compilers js:babel-register",
    "test": "mocha --compilers js:babel-register --recursive ./server/test --exit",
    "cover": "istanbul cover ./node_modules/.bin/_mocha -- server/test --exit",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "test-watch": "nodemon --exec \"npm test\"",
    "build": "babel --presets airbnb server -d compiled/",
    "build2": "babel --presets airbnb server -d compiled/",
    "build3": "babel --presets airbnb styles -d public/js"
  },
  "author": "Akanmu Christopher",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.3",    
    "babel-cli": "^6.26.0",
