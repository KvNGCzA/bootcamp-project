{
  "name": "final-project",
  "version": "1.0.0",
  "description": "Two week bootcamp project",
  "main": "server.js",
  "scripts": {
    "start": "npm server/server.js",
    "test": "mocha server/server.test.js",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha server/server.test.js",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "test-watch": "nodemon --exec \"npm test\"",
    "build": "babel --presets airbnb es6/es6styles -d public/js",
    "build2": "babel --presets airbnb es6/es6server -d server/",
    "build3": "babel --presets airbnb es6/es6models -d api/models",
    "build4": "babel --presets airbnb es6/es6routes -d api/routes"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "hbs": "^4.0.1",
    "morgan": "^1.9.0",
    "multer": "^1.3.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-airbnb": "^2.5.3",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.3.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "supertest": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KvNGCzA/bootcamp-project.git"
  },
  "bugs": {
    "url": "https://github.com/KvNGCzA/bootcamp-project/issues"
  },
  "homepage": "https://github.com/KvNGCzA/bootcamp-project#readme"
}
